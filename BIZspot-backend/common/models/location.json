{
  "name": "location",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "number": {
      "type": "number"
    },
    "street": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasAndBelongsToMany",
      "model": "user",
      "foreignKey": "userLocationId",
      "options": {
        "nestRemoting": true
      }
    },
    "businesses": {
      "type": "hasAndBelongsToMany",
      "model": "business",
      "foreignKey": "locationId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
